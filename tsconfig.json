{
    "compilerOptions": {
        "resolveJsonModule": true,
        "allowJs": true,
        "noEmit": true,
        "target": "es6",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "module": "CommonJS",
        "strict": true,
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "cypress",
            "node"
        ],
    },
    "include": [
        "**/*.ts",
        "cypress/e2e/*spec.ts",
        "cypress/support/e2e.js"    
]
}